{
  "manifest_version": 2,

  "name": "Hashpass",
  "description": "A simple, stateless password manager.",
  "version": "1.9.13",
  "permissions": [
    "activeTab",
    "tabs",
    "http://*/*",
    "https://*/*",
    "chrome://favicon/",
    "storage"
  ],
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },
  "browser_action": {
    "default_icon": {
      "19":  "icon.png",
      "38":  "icon.png",
      "128": "icon.png"
    },
    "default_popup": "popup.html"
  },
  "icons":
    {
      "19":  "icon.png",
      "38":  "icon.png",
      "128": "icon.png"
    }
}
